@article{smith_parallel_2010,
	title = {Parallel processing of visual space by neighboring neurons in mouse visual cortex},
	volume = {13},
	copyright = {© 2010 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1097-6256},
	url = {https://www.nature.com/neuro/journal/v13/n9/full/nn.2620.html},
	doi = {10.1038/nn.2620},
	abstract = {Visual cortex shows smooth retinotopic organization on the macroscopic scale, but it is unknown how receptive fields are organized at the level of neighboring neurons. This information is crucial for discriminating among models of visual cortex. We used in vivo two-photon calcium imaging to independently map ON and OFF receptive field subregions of local populations of layer 2/3 neurons in mouse visual cortex. Receptive field subregions were often precisely shared among neighboring neurons. Furthermore, large subregions seem to be assembled from multiple smaller, non-overlapping subregions of other neurons in the same local population. These experiments provide, to our knowledge, the first characterization of the diversity of receptive fields in a dense local network of visual cortex and reveal elementary units of receptive field organization. Our results suggest that a limited pool of afferent receptive fields is available to a local population of neurons and reveal new organizational principles for the neural circuitry of the mouse visual cortex.},
	language = {en},
	number = {9},
	urldate = {2017-06-19},
	journal = {Nature Neuroscience},
	author = {Smith, Spencer L. and Häusser, Michael},
	month = sep,
	year = {2010},
	keywords = {Neuronal physiology, Sensory systems},
	pages = {1144--1149},
	file = {Snapshot:/home/maximilian/.mozilla/firefox/mwad0hks.default/zotero/storage/5ZUJRMJ8/nn.2620.html:text/html}
}

@article{maruyama_detecting_2014,
	title = {Detecting cells using non-negative matrix factorization on calcium imaging data},
	volume = {55},
	issn = {0893-6080},
	url = {http://www.sciencedirect.com/science/article/pii/S0893608014000707},
	doi = {10.1016/j.neunet.2014.03.007},
	abstract = {We propose a cell detection algorithm using non-negative matrix factorization (NMF) on Ca2+ imaging data. To apply NMF to Ca2+ imaging data, we use the bleaching line of the background fluorescence intensity as an a priori background constraint to make the NMF uniquely dissociate the background component from the image data. This constraint helps us to incorporate the effect of dye-bleaching and reduce the non-uniqueness of the solution. We demonstrate that in the case of noisy data, the NMF algorithm can detect cells more accurately than Mukamel’s independent component analysis algorithm, a state-of-art method. We then apply the NMF algorithm to Ca2+ imaging data recorded on the local activities of subcellular structures of multiple cells in a wide area. We show that our method can decompose rapid transient components corresponding to somas and dendrites of many neurons, and furthermore, that it can decompose slow transient components probably corresponding to glial cells.},
	urldate = {2017-06-19},
	journal = {Neural Networks},
	author = {Maruyama, Ryuichi and Maeda, Kazuma and Moroda, Hajime and Kato, Ichiro and Inoue, Masashi and Miyakawa, Hiroyoshi and Aonishi, Toru},
	month = jul,
	year = {2014},
	keywords = {Non-negative matrix factorization, Multi-cellular calcium imaging, Independent component analysis, Semi-automatic cell detection, Dendrite},
	pages = {11--19},
	file = {ScienceDirect Full Text PDF:/home/maximilian/.mozilla/firefox/mwad0hks.default/zotero/storage/9CZUWVZ7/Maruyama et al. - 2014 - Detecting cells using non-negative matrix factoriz.pdf:application/pdf;ScienceDirect Snapshot:/home/maximilian/.mozilla/firefox/mwad0hks.default/zotero/storage/R38HS233/S0893608014000707.html:text/html}
